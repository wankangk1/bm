{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "eslint-config-standard-with-typescript", "prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.eslint.json"
  },
  "plugins": ["react", "@typescript-eslint", "prettier"],
  "rules": { "prettier/prettier": "error" }
}
